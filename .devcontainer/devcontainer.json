{
	"name": "techlog",
	"build": {
		"dockerfile": "Dockerfile"
	},
	"customizations": {
		"vscode": {
			"extensions": [
				"ms-ceintl.vscode-language-pack-ja",
				"mhutchie.git-graph",
				"glenn2223.live-sass"
			],
			"settings": {
				"editor.formatOnSave": true,
				"editor.tabSize": 2,
				"editor.wordWrapColumn": 200,
				"editor.wordWrap": "wordWrapColumn",
				"files.trimTrailingWhitespace": true,
				"git.path": "/usr/bin/git"
			}
		}
	}
}